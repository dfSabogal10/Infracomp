<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Esqueleto_VideoAndes_Nivel1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- SERVLETS -->
 
 <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>index.html</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>ServletPlantilla</servlet-name>
    <servlet-class>co.edu.uniandes.videoAndes.servlets.ServletPlantilla</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>InicioSesion</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.AdminServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>BotonesAdmin</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.ServletBotonesAdmin</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>RespuestaAdmin</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.ServletRespuestaAdmin</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>BotonesCliente</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.ServletBotonesCliente</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>BotonesGerente</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.ServletBotonesGerente</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>RespuestaGerente</servlet-name>
    <servlet-class>co.edu.uniandes.iteracion2.servlets.ServletRespuestaGerente</servlet-class>
  </servlet>
  
  <!--MAPPING-->
  
   <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ServletPlantilla</servlet-name>
    <url-pattern>/videosdefault.htm</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
    <servlet-name>InicioSesion</servlet-name>
    <url-pattern>/inicioSesion.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>BotonesAdmin</servlet-name>
    <url-pattern>/botonesAdmin.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>RespuestaAdmin</servlet-name>
    <url-pattern>/RespuestaAdmin.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>BotonesCliente</servlet-name>
    <url-pattern>/botonesCliente.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>BotonesGerente</servlet-name>
    <url-pattern>/botonesGerente.htm</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>RespuestaGerente</servlet-name>
    <url-pattern>/RespuestaGerente.htm</url-pattern>
  </servlet-mapping>
</web-app>
